◊(require racket/list)

◊(define (explode lexicon)
    (apply
      string-append
    (flatten
      (for/list ([e lexicon])
        (let ([lemma (car e)]
              [poses (cdr e)])
          (for/list ([pos poses])
            (string-append
	     lemma
             " "
	     (symbol->string pos)
             " ;\n")))))))

